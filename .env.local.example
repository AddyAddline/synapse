# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenRouter (LLM)
OPENROUTER_API_KEY=your_openrouter_api_key

# Judge0 CE (Code Execution)
# Self-hosted: point to your GCP VM running the Judge0 stack
#   JUDGE0_API_URL=http://<VM_IP>:2358
#   JUDGE0_API_KEY=<token from infrastructure/judge0.conf>
# Hosted (rate-limited ~50 req/day):
#   JUDGE0_API_URL=https://judge0-ce.p.sulu.sh
JUDGE0_API_URL=your_judge0_url
JUDGE0_API_KEY=your_judge0_api_key
# Octave language ID â€” 32 for hosted Sulu API, 66 for self-hosted Judge0 latest
JUDGE0_OCTAVE_LANG_ID=66
